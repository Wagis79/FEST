{
  "name": "fest-nutrient-decision-support",
  "version": "2.8.1",
  "description": "Intelligent beslutsstöd för gödselrekommendationer med inköpslista och balanshantering",
  "scripts": {
    "start": "node --max-old-space-size=4096 node_modules/tsx/dist/cli.mjs src/api/start.ts",
    "server": "node --max-old-space-size=12288 node_modules/tsx/dist/cli.mjs src/api/start.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "smoke:admin": "tsx src/api/smoke-admin.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "nutrient",
    "fertilizer",
    "recommendation",
    "agriculture",
    "shopping-list",
    "balance-calculation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.16",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "highs": "^1.8.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.4"
  }
}
